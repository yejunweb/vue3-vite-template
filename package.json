{
    "name": "vue3-vite-template",
    "version": "1.0.0",
    "license": "GPL-3.0-only",
    "scripts": {
        "dev": "vite",
        "serve": "npm run dev",
        "serve:stage": "vite serve --mode staging",
        "serve:prod": "vite serve --mode production",
        "build": "vite build",
        "build:stage": "vite build --mode staging",
        "build:prod": "vite build --mode production",
        "report": "rimraf dist && vite build",
        "preview": "npm run build && vite preview",
        "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
        "lint:prettier": "prettier . --write  --ignore-unknown",
        "lint:stylelint": "stylelint --fix './src/**/*.{css,scss,less,vue}' --cache --cache-location node_modules/.cache/stylelint/",
        "lint:eslint": "eslint ./src --fix",
        "lint:ls-lint": "ls-lint",
        "commit": "git-cz",
        "prepare": "husky install"
    },
    "dependencies": {
        "@types/crypto-js": "4.2.2",
        "@types/lodash": "4.17.20",
        "@vueuse/core": "10.11.0",
        "ant-design-vue": "4.x",
        "axios": "1.7.2",
        "crypto-js": "4.2.0",
        "dayjs": "1.11.11",
        "echarts": "5.5.1",
        "lodash": "4.17.21",
        "pinia": "2.1.7",
        "vue": "3.4.31",
        "vue-router": "4.4.0"
    },
    "devDependencies": {
        "@commitlint/cli": "19.2.2",
        "@commitlint/config-conventional": "19.2.2",
        "@ls-lint/ls-lint": "2.0.0",
        "@rushstack/eslint-patch": "1.10.3",
        "@tsconfig/node18": "18.2.4",
        "@types/node": "18.16.18",
        "@unocss/eslint-config": "0.61.0",
        "@vitejs/plugin-legacy": "5.4.1",
        "@vitejs/plugin-vue": "5.0.5",
        "@vitejs/plugin-vue-jsx": "4.0.0",
        "@vue/eslint-config-prettier": "9.0.0",
        "@vue/eslint-config-typescript": "13.0.0",
        "@vue/tsconfig": "0.5.1",
        "autoprefixer": "10.4.19",
        "commitizen": "4.3.0",
        "cssnano": "7.0.3",
        "cz-conventional-changelog": "3.3.0",
        "cz-customizable": "7.0.0",
        "eslint": "8.57.0",
        "eslint-plugin-vue": "9.27.0",
        "husky": "9.0.11",
        "lint-staged": "11.1.2",
        "postcss": "8.4.39",
        "postcss-html": "1.8.0",
        "postcss-import": "16.1.0",
        "prettier": "3.3.2",
        "rimraf": "5.0.7",
        "rollup-plugin-visualizer": "5.12.0",
        "sass": "1.77.6",
        "stylelint": "16.21.1",
        "stylelint-config-recess-order": "7.1.0",
        "stylelint-config-recommended-scss": "15.0.1",
        "stylelint-config-recommended-vue": "1.6.1",
        "stylelint-config-standard": "38.0.0",
        "stylelint-order": "7.0.0",
        "stylelint-prettier": "5.0.3",
        "stylelint-scss": "6.12.1",
        "typescript": "5.5.3",
        "unocss": "0.61.0",
        "unplugin-auto-import": "0.17.6",
        "unplugin-vue-components": "0.27.2",
        "vite": "5.3.3",
        "vite-plugin-compression2": "1.1.2",
        "vite-plugin-env-types": "0.1.4",
        "vite-plugin-inspect": "0.8.4",
